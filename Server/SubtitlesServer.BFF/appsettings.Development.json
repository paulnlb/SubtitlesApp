{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "whisper",
        "Match": {
          "Path": "{**catchall}"
        }
      }
    },
    "Clusters": {
      "whisper": {
        "LoadBalancingPolicy": "LeastRequests",
        "Destinations": {
          "whisper1": {
            "Address": "http://localhost:5032"
          },
          "whisper2": {
            "Address": "http://localhost:5033"
          }
        }
      }
    }
  }
}
